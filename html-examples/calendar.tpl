			<tr class="Calendar-body-line">
				<th class="Calendar-body-head">Пн</th>
				<th class="Calendar-body-head">Вт</th>
				<th class="Calendar-body-head">Ср</th>
				<th class="Calendar-body-head">Чт</th>
				<th class="Calendar-body-head">Пт</th>
				<th class="Calendar-body-head">Сб</th>
